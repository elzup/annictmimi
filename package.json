{
	"name": "annictmimi",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://annictmimi.elzup.org",
	"scripts": {
		"start": "node scripts/start.js",
		"offbuild": "node scripts/build.js",
		"build": ". .env.production && node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"deploy": "npm run build && gh-pages -d build",
		"coveralls":
			"node scripts/test.js --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"flow": "flow",
		"gen:env":
			"cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"annict": "0.0.9",
		"camelcase-keys-recursive": "0.8.3",
		"graphql": "0.13.2",
		"lodash": "4.17.15",
		"material-ui": "1.0.0-beta.46",
		"material-ui-icons": "1.0.0-beta.36",
		"moment": "2.24.0",
		"normalizr": "3.4.1",
		"query-string": "5.1.1",
		"react": "16.10.2",
		"react-addons-css-transition-group": "15.6.2",
		"react-dom": "16.10.2",
		"react-image": "1.5.1",
		"react-redux": "5.1.2",
		"react-router-dom": "4.3.1",
		"redux": "3.7.2",
		"redux-persist": "5.10.0",
		"redux-thunk": "2.3.0",
		"style-loader": "0.23.1",
		"styled-components": "3.4.10",
		"superagent": "3.8.3",
		"sw-precache-webpack-plugin": "0.11.5",
		"url-loader": "0.6.2",
		"webpack": "3.12.0",
		"webpack-dev-server": "3.8.2",
		"webpack-manifest-plugin": "1.3.2",
		"whatwg-fetch": "2.0.4"
	},
	"devDependencies": {
		"@babel/cli": "7.6.4",
		"@babel/preset-env": "7.6.3",
		"@storybook/addon-actions": "3.4.12",
		"@storybook/addon-links": "3.4.12",
		"@storybook/addons": "3.4.12",
		"@storybook/react": "3.4.12",
		"autoprefixer": "8.6.5",
		"babel-core": "6.26.3",
		"babel-eslint": "8.2.1",
		"babel-jest": "22.4.4",
		"babel-loader": "7.1.5",
		"babel-plugin-lodash": "3.3.4",
		"babel-plugin-s2s-variable-initializing": "0.1.11",
		"babel-preset-react-app": "3.1.2",
		"babel-runtime": "6.26.0",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"chalk": "2.4.2",
		"coveralls": "3.0.7",
		"css-loader": "0.28.11",
		"dotenv": "5.0.1",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.15.1",
		"enzyme-to-json": "3.4.3",
		"eslint": "4.19.1",
		"eslint-config-react-app": "2.1.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "2.43.0",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jsx-a11y": "5.1.1",
		"eslint-plugin-react": "7.16.0",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "1.1.11",
		"flow-bin": "0.110.0",
		"fs-extra": "5.0.0",
		"gh-pages": "1.2.0",
		"html-webpack-plugin": "2.30.1",
		"husky": "0.14.3",
		"jest": "22.4.4",
		"lint-staged": "6.1.1",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.3.1",
		"postcss-loader": "2.1.6",
		"prettier": "1.18.2",
		"promise": "8.0.3",
		"raf": "3.4.1",
		"react-dev-utils": "5.0.3",
		"react-test-renderer": "16.10.2",
		"s2s": "0.20.1",
		"s2s-plugins-redux": "0.4.8"
	},
	"lint-staged": {
		"src/**/*.js": ["prettier --config ./.prettierrc --write", "git add"]
	},
	"jest": {
		"collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
		"setupFiles": ["<rootDir>/config/polyfills.js"],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)":
				"<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"mjs",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	}
}
